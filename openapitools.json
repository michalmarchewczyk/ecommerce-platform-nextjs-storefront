{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.2.0",
    "generators": {
      "v3.0": {
        "generatorName": "typescript-fetch",
        "output": "./lib/api/client",
        "inputSpec": "https://github.com/michalmarchewczyk/ecommerce-platform-nestjs-api/releases/download/v1.4.22/specification.json",
        "reservedWordsMappings": "return=return",
        "additionalProperties": {
          "withoutRuntimeChecks": true
        }
      }
    }
  }
}
